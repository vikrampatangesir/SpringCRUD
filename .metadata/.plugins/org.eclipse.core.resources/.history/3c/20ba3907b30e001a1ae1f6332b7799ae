package com.softedge.main;

import javax.management.Notification;


import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.beans.factory.annotation.Qualifier;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
public class Mycontroller
{

	User user=new User("vikram", 20, "vikram@gmail.com");
	
	@Autowired
	@Qualifier
	INotificationService abc;
	@Autowired
	INotificationService SMSNotificationService;
	
	
	
	/*@Autowired
	NotificationService notification;
	@Autowired
	NotificationService notification1;*/
	
	
    @GetMapping("/test")
	public User getGreeting()
	{
    	SMSNotificationService.sendMsgString("vikram", "123");
    	abc.sendMsgString("john","456" );
    	return  user;
    	
		
	}
    
    

}
